package coreJava;

public class Fibonacci extends Scanner1 {

	
	public static void main(String[] args)
	
	{
		Fibonacci fb=new Fibonacci();
		scan();
		int i;
		int j;
		int res=0;
		int temp = 0;
		for(i=0;i<=num;i++)
		{
			for (j = 1; j <= i; j++) {
				//System.out.print(j);
			}
			//temp++;
			System.out.println(i);
			System.out.print(i+j);
			
		}
		i=i=j;
		//System.out.print(temp+""+i);
	}
}

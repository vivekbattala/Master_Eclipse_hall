package coreJava;

public class Fibonacci extends Scanner1 {

	
	public static void main(String[] args)
	
	{
		Fibonacci fb=new Fibonacci();
		scan();
		int i;
		int res=0;
		int temp = 0;
		int temp1 = 1;
		for(i=0;i<=num;i++)
		{
			System.out.println(temp);
			res=temp+temp1;
			temp=temp1;
			temp1=res;
	}
}
}

package coreJava;

public class Fibonacci extends Scanner1 {

	
	public static void main(String[] args)
	
	{
		Fibonacci fb=new Fibonacci();
		scan();
		int i;
		int res=0;
		int temp = 0;
		for(i=0;i<=num;i++)
		{
			for (int j = 1; j >= i; j++) {
				System.out.print(j);
			}
			temp++;
			System.out.print(i);
			
		}
		
		//System.out.print(temp+""+i);
	}
}
